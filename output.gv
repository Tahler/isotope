digraph {
  node [
    fontsize = "16"
    shape = plaintext
  ];

  B [label=<
<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
  <TR><TD><B>B</B><BR />CPU: 10.00%<BR />RAM: 10.00%<BR />Err: 0.00%</TD></TR>
</TABLE>>];

  C [label=<
<TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0">
  <TR><TD><B>C</B><BR />CPU: 10.00%<BR />RAM: 10.00%<BR />Err: 0.00%</TD></TR>
